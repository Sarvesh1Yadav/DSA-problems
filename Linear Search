#include<iostream>
#include<stdlib.h>
using namespace std;
int main()
{ int arr[10],i,n,num;
  cout<<"Enter length of arr: ";
  cin>>n;
  cout<<"Enter no to be searched: ";
  cin>>num;
  for(int i=0;i<n;i++){
  	cin>>arr[i];
  }
  for(int i=0;i<n;i++)
  {
   	if(arr[i]==num)
   	{
	   
   	cout<<"Number is found at " << i <<" index";
   	exit(1);
   }
  } 
cout<<"Number is not found";
}

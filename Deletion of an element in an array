#include<stdio.h>
int main()
{ int n,i,m;
	int arr[50];
	printf("enter length");
	scanf("%d",&n);
	printf("enter value");
	for(i=0;i<n;i++)
	scanf("%d",&arr[i]);
	printf("enter index for deletion\n");
	scanf("%d",&m);
	for(i=m;i<n-1;i++)
	arr[i]=arr[i+1];
	for(i=0;i<n-1;i++)
	printf(" %d",arr[i]);
	}
